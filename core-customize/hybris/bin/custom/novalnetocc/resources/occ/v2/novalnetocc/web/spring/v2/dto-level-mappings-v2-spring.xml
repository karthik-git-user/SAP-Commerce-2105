<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

    <alias name="defaultNovalnetNnResponseDTOFieldSetLevelMapping" alias="novalnetNnResponseWsDTOFieldSetLevelMapping"/>
    <bean parent="fieldSetLevelMapping" id="defaultnovalnetNnResponseDTOFieldSetLevelMapping">
        <property name="dtoClass"
                  value="novalnet.dto.payment.nnResponseWsDTO"/>
        <property name="levelMapping">
            <map>
                <entry key="BASIC"
                       value="redirectURL"/>
                <entry key="DEFAULT"
                       value="redirectURL"/>
                <entry key="FULL"
                       value="redirectURL"/>
            </map>
        </property>
    </bean>

</beans>
