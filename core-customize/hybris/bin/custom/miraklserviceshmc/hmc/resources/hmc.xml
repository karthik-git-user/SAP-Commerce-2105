    <type name="Offer" mode="append">
        <organizer>
            <search>
                <condition attribute="id"/>
                <condition attribute="shop"/>
                <condition attribute="productCode"/>
                <condition attribute="active"/>
                <condition attribute="state"/>
                <condition attribute="availableStartDate"/>
                <condition attribute="availableEndDate"/>
            </search>
            <result mode="replace">
                <listview>
                    <itemlayout>
                        <attribute name="id" width="50"/>
                        <attribute name="shop" width="150"/>
                        <attribute name="productCode" width="150"/>
                        <attribute name="active" width="150"/>
                        <attribute name="state" width="150"/>
                        <attribute name="deleted" width="150"/>
                        <attribute name="availableStartDate" width="150"/>
                        <attribute name="availableEndDate" width="150"/>
                        <attribute name="minOrderQuantity" width="150"/>
                        <attribute name="availableEndDate" width="150"/>
                        <attribute name="packageQuantity" width="150"/>
                    </itemlayout>
                </listview>
            </result>
            <editor>
                <essentials>
                    <columnlayout>
                        <row>
                            <attribute name="id"/>
                            <attribute name="productCode"/>
                        </row>
                        <row>
                            <attribute name="active"/>
                            <attribute name="state"/>
                        </row>
                        <row>
                            <attribute name="availableStartDate"/>
                            <attribute name="availableEndDate"/>
                        </row>
                        <row>
                            <attribute name="shop"/>
                            <attribute name="currency"/>
                        </row>
                    </columnlayout>
                </essentials>

                <tab name="tab.offer.properties" position="1">
                    <section name="hmc.offer.general">
                        <listlayout>
                            <attribute name="quantity"/>
                            <attribute name="favoriteRank"/>
                            <attribute name="leadTimeToShip"/>
                            <attribute name="description"/>
                            <attribute name="deleted"/>
                            <attribute name="customFieldsJSON"/>
                        </listlayout>
                    </section>
                    <section name="hmc.offer.prices">
                        <columnlayout>
                            <row>
                                <attribute name="price"/>
                                <attribute name="totalPrice"/>
                            </row>
                            <row>
                                <attribute name="minShippingPrice"/>
                                <attribute name="minShippingPriceAdditional"/>
                            </row>
                            <row>
                                <attribute name="originPrice"/>
                                <attribute name="priceAdditionalInfo"/>
                            </row>
                            <row>
                                <attribute name="allOfferPricingsJSON"/>
                            </row>
                        </columnlayout>
                    </section>
                    <section name="hmc.offer.discount">
                        <columnlayout>
                            <row>
                                <attribute name="discountPrice"/>
                            </row>
                            <row>
                                <attribute name="discountStartDate"/>
                                <attribute name="discountEndDate"/>
                            </row>
                        </columnlayout>
                    </section>
                </tab>
            </editor>
        </organizer>
    </type>

    <type name="MiraklExportSellableProductsCronJob" mode="append">
