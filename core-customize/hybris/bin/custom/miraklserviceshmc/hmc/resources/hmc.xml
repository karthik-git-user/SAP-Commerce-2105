    <explorertree>
        <group name="mirakl">
            <group name="mirakl.jobs">
                <typeref type="MiraklExportSellableProductsCronJob" description="typeref.miraklexportsellableproductscronjob.description"/>
                <typeref type="MiraklImportCustomFieldsCronJob" description="typeref.miraklimportcustomfieldscronjob.description"/>
                <typeref type="MiraklImportShopOfferStatesCronJob" description="typeref.miraklimportshopofferstatescronjob.description"/>
		        <typeref type="MiraklImportShopOffersCompositeCronJob" description="typeref.miraklimportshopofferscompositecronjob.description"/>
                <typeref type="MiraklRetrieveConsignmentUpdatesCronJob" description="typeref.miraklretrieveconsignmentupdatescronjob.description"/>
                <typeref type="MiraklRetrieveProcessDebitsCompositeCronJob" description="typeref.miraklretrieveprocessdebitscompositecronjob.description"/>
                <typeref type="MiraklRetrieveProcessRefundsCompositeCronJob" description="typeref.miraklretrieveprocessrefundscompositecronjob.description"/>
                <typeref type="MiraklExportCatalogCronJob" description="typeref.miraklexportcatalogcronjob.description"/>
                <typeref type="MiraklDownloadProductFilesCronjob" description="typeref.mirakldownloadproductfilescronjob.description"/>
                <typeref type="MiraklProductImportCronJob" description="typeref.miraklimportproductscronjob.description"/>
                <typeref type="MiraklProductImportCompositeCronJob" description="typeref.miraklproductimportcompositecronjob.description"/>
                <typeref type="MiraklCleanupRawProductsCronjob" description="typeref.miraklcleanuprawproductscronjob.description"/>
                <typeref type="MiraklExportStatusCronJob" description="typeref.miraklexportstatuscronjob.description"/>
            </group>
            <typeref type="Offer" description="typeref.offer.description"/>
			<typeref type="Shop"/>
            <typeref type="MiraklCoreAttribute" description="typeref.miraklcoreattribute.description"/>
            <typeref type="MiraklCoreAttributeConfiguration" description="typeref.miraklcoreattributeconfiguration.description"/>
            <typeref type="MiraklRawProduct" description="typeref.miraklrawproduct.description"/>
            <typeref type="MiraklJobReport" description="typeref.miraklJobReport.description"/>
            <typeref type="MiraklEnvironment" description="typeref.miraklenvironment.description"/>
            <typeref type="MiraklCustomField" description="typeref.miraklcustonfield.description"/>
        </group>

        <group name="order" expand="">
            <typeref type="MarketplaceConsignment" description="typeref.marketplaceConsignment.description" position="4"/>
        </group>
    </explorertree>

	<type name="Shop" mode="append">
		<organizer>
			<search>
				<condition attribute="id" operator="contains" />
				<condition attribute="name" operator="contains" />
				<condition attribute="state" operator="equal" />
			</search>
			<result>
				<listview mode="replace">
					<itemlayout>
						<attribute name="id" width="120" />
						<attribute name="name" width="180" />
						<attribute name="state" width="180" />
					</itemlayout>
				</listview>
				<editview>
					<itemlayout>
						<attribute name="id" width="120" />
						<attribute name="name" width="180" />
						<attribute name="state" width="180" />
					</itemlayout>
				</editview>
				<treeview>
					<itemlayout>
						<attribute name="id" width="120" />
						<attribute name="name" width="180" />
						<attribute name="state" width="180" />
					</itemlayout>
				</treeview>
			</result>
			<editor>
				<essentials>
					<columnlayout>
						<row>
							<attribute name="id" />
							<attribute name="name" />
						</row>
					</columnlayout>
				</essentials>
				<tab name="properties" position="0">
					<section name="properties">
						<listlayout>
							<attribute name="description" width="595" />
							<attribute name="internalId" width="595" />
							<attribute name="currency" width="595" />
							<attribute name="state" width="595" />
							<attribute name="professional" width="595" />
							<attribute name="premium" width="595" />
							<attribute name="premiumState" width="595" />
							<attribute name="approvalDelay" width="595" />
							<attribute name="approvalRate" width="595" />
							<attribute name="evaluationCount" width="595" />
							<attribute name="logo" width="595" >
								<mediaeditor />
							</attribute>
							<attribute name="banner" width="595" >
								<mediaeditor />
							</attribute>
							<attribute name="freeShipping" width="595" />
                            <attribute name="customFieldsJSON" width="595" />
						</listlayout>
					</section>
				</tab>
			</editor>
		</organizer>
		<defaultreference searchattribute="id" mode="replace">
			<itemlayout>
				<attribute name="id" />
				<attribute name="name" />
				<attribute name="state" />
			</itemlayout>
		</defaultreference>
	</type>

	<type name="MiraklImportShopsCronjob" mode="append">
