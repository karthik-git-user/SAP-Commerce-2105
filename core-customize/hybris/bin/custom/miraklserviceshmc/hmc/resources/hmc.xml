    <type name="MiraklJobReport" mode="append">
        <organizer>
            <search>
                <condition attribute="jobId"/>
                <condition attribute="status"/>
                <condition attribute="reportType"/>
                <condition attribute="hasErrorReport"/>
            </search>
            <result mode="replace">
                <listview>
                    <itemlayout>
                        <attribute name="creationDate" width="150"/>
                        <attribute name="jobId" width="50"/>
                        <attribute name="status" width="70"/>
                        <attribute name="reportType" width="150"/>
                        <attribute name="hasErrorReport" width="70"/>
                    </itemlayout>
                </listview>
            </result>
            <editor>
                <essentials>
                    <columnlayout>
                        <row>
                            <attribute name="jobId"/>
                            <attribute name="status"/>
                        </row>
                        <row>
                            <attribute name="reportType"/>
                            <attribute name="hasErrorReport"/>
                        </row>
                        <row>
                            <attribute name="creationDate"/>
                        </row>
                    </columnlayout>
                </essentials>
                <tab name="tab.miraklJobReport.properties" position="1">
                    <section name="hmc.mirakljobreport.items">
                        <listlayout>
                            <attribute name="itemsUpdated"/>
                            <attribute name="itemsInserted"/>
                            <attribute name="itemsDeleted"/>
                        </listlayout>
                    </section>
                    <section name="hmc.mirakljobreport.lines">
                        <listlayout>
                            <attribute name="linesRead"/>
                            <attribute name="linesInSuccess"/>
                            <attribute name="linesInError"/>
                        </listlayout>
                    </section>
                </tab>
                <tab name="tab.mirakljobreport.errors" position="2">
                    <section name="hmc.mirakljobreport.errorreport">
                        <listlayout>
                            <attribute name="errorReportPreview" width="300">
                                <textareaeditor expanded="true" wrap="false" cols="200" rows="20"/>
                            </attribute>
                            <attribute name="errorReport"/>
                        </listlayout>
                    </section>
                </tab>
            </editor>
        </organizer>
    </type>

