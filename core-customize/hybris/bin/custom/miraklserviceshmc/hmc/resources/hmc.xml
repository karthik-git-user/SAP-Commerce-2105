    <type name="MiraklCoreAttributeConfiguration" mode="append">
        <organizer>
            <search>
                <condition attribute="code" operator="contains"/>
            </search>
            <result mode="replace">
                <listview>
                    <itemlayout>
                        <attribute name="code" width="200"/>
                        <attribute name="productRootCategoryCode" width="200"/>
                    </itemlayout>
                </listview>
            </result>
            <editor>
                <essentials>
                    <listlayout mode="append">
                        <attribute name="code" mode="append"/>
                        <attribute name="productRootCategoryCode" mode="append" editable="false"/>
                        <attribute name="coreAttributes">
                            <referencecollectioneditor resortable="true" create="true" remove="true" autosave="false" editable="true">
                                <itemlayout>
                                    <attribute name="code"/>
                                    <attribute name="label"/>
                                    <attribute name="localized"/>
                                    <attribute name="variant"/>
                                    <attribute name="requirementLevel"/>
                                    <attribute name="type"/>
                                    <attribute name="typeParameter"/>
                                    <attribute name="role"/>
                                    <attribute name="category"/>
                                    <attribute name="importExportHandlerStringId"/>
                                    <attribute name="uniqueIdentifier"/>
                                    <attribute name="defaultValue"/>
                                </itemlayout>
