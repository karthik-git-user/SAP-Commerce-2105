    <type name="MarketplaceConsignment" mode="append">
        <organizer>
            <search mode="append">
                <condition attribute="code" operator="equal" />
                <condition attribute="shopId" operator="contains" />
                <condition attribute="shopName" operator="contains" />
                <condition attribute="miraklOrderStatus" operator="equal" />
                <condition attribute="paymentStatus" operator="equal" />
            </search>
            <result>
                <listview>
                    <itemlayout>
                        <attribute name="shopId" width="50" />
                        <attribute name="shopName" width="100" />
                        <attribute name="miraklOrderStatus" width="180" />
                        <attribute name="paymentStatus" width="100" />
                    </itemlayout>
                </listview>
            </result>
            <editor mode="append">
                <essentials>
                    <columnlayout>
                        <row>
                            <attribute name="shopId" editable="false"/>
                            <attribute name="shopName" editable="false"/>
                        </row>
                        <row>
                            <attribute name="miraklOrderStatus" editable="false"/>
                            <attribute name="paymentStatus" editable="false"/>
                        </row>
                        <row>
                            <attribute name="totalPrice" editable="false"/>
                            <attribute name="shippingCost" editable="false"/>
                        </row>
                    </columnlayout>
                </essentials>
                <tab name="properties" position="0" mode="append">
                    <section name="consignment.details">
                        <listlayout>
                            <attribute name="canCancel" editable="false"/>
                            <attribute name="canEvaluate" editable="false"/>
                            <attribute name="leadTimeToShip" width="70" editable="false"/>
                            <attribute name="imprintNumber" width="180" editable="false"/>
                            <attribute name="debitRequestPayload" width="300" editable="false"/>
                            <attribute name="consignmentUpdatePayload" width="300" editable="false"/>
                            <attribute name="lastUpdateProcessed" editable="false"/>
                            <attribute name="shippingTypeCode" width="50" editable="false"/>
                            <attribute name="shippingTypeLabel" width="100" editable="false"/>
                            <attribute name="customerDebitDate" editable="false"/>
                            <attribute name="customFieldsJSON" editable="false"/>
                        </listlayout>
                    </section>
                </tab>
            </editor>
        </organizer>
        <defaultreference>
            <itemlayout>
                <attribute name="shopId" />
                <attribute name="shopName" />
                <attribute name="miraklOrderStatus" />
            </itemlayout>
        </defaultreference>
    </type>

